{
    "name": "monolyth/reroute",
    "description": "Extremely flexible router for the Monolyth unframework",
    "license": "MIT",
    "keywords": ["php","mvc","router"],
    "authors": [
        {
            "name": "Marijn Ophorst",
            "email": "marijn@monomelodies.nl"
        }
    ],
    "require": {
        "php": ">=8.1",
        "jakeasmith/http_build_url": "^1.0.0",
        "league/pipeline": "^0.1.0",
        "psr/http-message": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Monolyth\\Reroute\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Monolyth\\Reroute\\": "src",
            "Monolyth\\Reroute\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "toast/unit": "^2.1",
        "gentry/gentry": "^0.13.12",
        "gentry/toast": "^0.1.1",
        "laminas/laminas-diactoros": "^2.2"
    }
}
